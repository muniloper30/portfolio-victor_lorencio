{
    "ci": {
        "collect": {
            "startServerCommand": "npm run serve:ssr:portfolio-victor",
            "url": [
                "http://localhost:4000/"
            ],
            "numberOfRuns": 3,
            "settings": {
                "preset": "desktop",
                "throttling": {
                    "rttMs": 40,
                    "throughputKbps": 10240,
                    "cpuSlowdownMultiplier": 1
                }
            }
        },
        "assert": {
            "assertions": {
                "categories:performance": [
                    "error",
                    {
                        "minScore": 0.9
                    }
                ],
                "categories:accessibility": [
                    "warn",
                    {
                        "minScore": 0.95
                    }
                ],
                "categories:best-practices": [
                    "warn",
                    {
                        "minScore": 0.95
                    }
                ],
                "categories:seo": [
                    "warn",
                    {
                        "minScore": 0.95
                    }
                ],
                "first-contentful-paint": [
                    "error",
                    {
                        "maxNumericValue": 2000
                    }
                ],
                "largest-contentful-paint": [
                    "error",
                    {
                        "maxNumericValue": 2500
                    }
                ],
                "cumulative-layout-shift": [
                    "error",
                    {
                        "maxNumericValue": 0.1
                    }
                ],
                "total-blocking-time": [
                    "warn",
                    {
                        "maxNumericValue": 300
                    }
                ]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}